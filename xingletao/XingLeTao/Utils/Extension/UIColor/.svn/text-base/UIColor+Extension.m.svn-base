#import "UIColor+Extension.h"

@implementation UIColor (Extension)
- (BOOL)sameColor:(UIColor*)color
{
    if (CGColorEqualToColor(self.CGColor, color.CGColor)) {
        return YES;
    }
    return NO;
}
@end
